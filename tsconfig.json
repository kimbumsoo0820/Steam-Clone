{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "jsx": "react",
    "sourceMap": true,
    "types": ["node"], // image태그의 src에 이미지 삽입 가능
    "resolveJsonModule": true, // json import 가능하게 해줌
  },
}